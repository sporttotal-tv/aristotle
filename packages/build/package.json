{
  "name": "@sporttotal/aristotle-build",
  "main": "./dist/index.js",
  "public": true,
  "version": "1.0.16",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "watch": "tsc --watch",
    "test": "ava --verbose"
  },
  "ava": {
    "files": [
      "test/**/index.ts",
      "test/*.ts"
    ],
    "concurrency": 1,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": "https://github.com/atelier-saulx/aristotle",
  "dependencies": {
    "@babel/generator": "^7.12.11",
    "@babel/parser": "^7.12.11",
    "@sporttotal/aristotle-types": "^1.0.0",
    "@sporttotal/get-package": "^1.0.7",
    "@sporttotal/utils": "^1.6.6",
    "autoprefixer": "^10.2.3",
    "chokidar": "^3.5.1",
    "cssnano": "^4.1.10",
    "esbuild": "^0.8.36",
    "exit-hook": "^2.2.0",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "mime-types": "^2.1.28",
    "murmurhash-native": "^3.5.0",
    "postcss": "^8.2.4",
    "postcss-default-unit": "^1.0.0",
    "postcss-flexbugs-fixes": "^5.0.2"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "react": "^17.0.1",
    "redis": "*",
    "ts-node": "^9.1.1"
  }
}
